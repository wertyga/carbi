{
  "name": "HTTP",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "http:dev": "cross-env NODE_ENV=development nodemon -w ./HTTP --exec babel-node ./HTTP/server.js",
    "io:dev": "cross-env NODE_ENV=development nodemon -w ./IO --exec babel-node ./IO/io-server.js",
    "bot:dev": "cross-env NODE_ENV=development nodemon -w ./BOT --exec babel-node ./BOT/bot-server.js",
    "collector:dev": "cross-env NODE_ENV=development nodemon -w ./COLLECTOR --exec babel-node ./COLLECTOR/server.js",
    "http:build": "cross-env NODE_ENV=production babel ./HTTP --out-dir BUILD/HTTP",
    "io:build": "cross-env NODE_ENV=production babel ./IO --out-dir BUILD/IO",
    "bot:build": "cross-env NODE_ENV=production babel ./BOT --out-dir BUILD/BOT",
    "collector:build": "cross-env NODE_ENV=production babel ./COLLECTOR --out-dir BUILD/COLLECTOR",
    "all:build": "npm run http:build && npm run io:build && npm run bot:build && npm run collector:build && babel envs --out-dir BUILD/envs && babel logger --out-dir BUILD/logger",
    "http:prod:start": "node BUILD/HTTP/server.js",
    "io:prod:start": "node BUILD/IO/io-server.js",
    "bot:prod:start": "node BUILD/BOT/bot-server.js",
    "collector:prod:start": "node BUILD/COLLECTOR/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@types/express": "^4.17.9",
    "axios": "^0.19.2",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "lodash": "^4.17.20",
    "mongoose": "^5.11.2",
    "node-telegram-bot-api": "^0.40.0",
    "nodemon": "^2.0.6",
    "react-ga": "^2.7.0",
    "short-id": "0.1.0-1",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/node": "^7.12.6",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "babel-plugin-module-resolver": "^4.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.14.0"
  }
}
